<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="TOP_RIGHT" spacing="15.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="com.briksoftware.updatefx.gui.UpdateController">
   <children>
      <Label fx:id="stepLabel" prefWidth="350.0" />
      <ProgressBar fx:id="progressBar" prefHeight="20.0" prefWidth="350.0" progress="0.0" />
      <Label fx:id="progressLabel" alignment="CENTER_RIGHT" prefWidth="350.0" />
      <Button fx:id="actionButton" cancelButton="true" mnemonicParsing="false" onAction="#cancel" text="%button.cancel">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
